nilai = int(input('masukkan nilai ='))

if nilai >=80 and nilai >= 100  :
  print('A')
elif nilai >= 70 and nilai <80  :
  print('B')
elif nilai >= 60 and nilai <70  :
  print('C')
elif nilai >= 40 and nilai <60  :
  print('D')
elif nilai <40 and nilai >= 0  :
  print('E')
else :
  print('invalid')
